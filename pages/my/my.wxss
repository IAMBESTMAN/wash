}
.my_indent {
	width: 100%;
	background: #fff;
	padding: 26rpx;
	margin-top: 20rpx;
	box-sizing: border-box;
}
.indent_title {
	display: flex;
	justify-content: space-between;
	font-size: 28rpx;
	color: #262626;
	padding-bottom: 23rpx;
	/*border-bottom: 1px solid #ebecef;*/
}
.see_all {
	color: #bdbdbd;
}
.my_list {
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	overflow: hidden;
	border-top: 1px solid #ebecef;
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.my_list > image{
	/*float: left;*/
