	font-size: 24rpx;
	float: right;
}
.list_icon {
	font-size: 28rpx;
	/*color: #ebecef;*/
	margin-left: 20rpx;
}
.lamp_content {
	padding: 0 30rpx;
	color: #262626;
	font-size: 28rpx;
	display: flex;
	align-items: center;
}
.lamp_content image {
